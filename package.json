{
  "name": "image-gallery",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "commit": "git-cz",
    "major": "standard-version -r major --preset gitmoji-config",
    "minor": "standard-version -r minor --preset gitmoji-config",
    "patch": "standard-version -r patch --preset gitmoji-config"
  },
  "dependencies": {
    "vue": "^3.2.47",
    "vuetify": "^3.1.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.5.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "commitizen": "^4.3.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "conventional-changelog-gitmoji-config": "^1.4.7",
    "cz-custom": "^0.0.2",
    "husky": "^8.0.3",
    "standard-version": "^9.5.0",
    "vite": "^4.2.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-custom"
    }
  }
}
